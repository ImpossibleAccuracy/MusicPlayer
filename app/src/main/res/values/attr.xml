    <declare-styleable name="PageView">
        <attr name="circle_color" format="color" />
        <attr name="selected_circle_color" format="color" />
        <attr name="circle_radius" format="dimension" />
        <attr name="circle_margins" format="dimension" />
    </declare-styleable>

</resources>
